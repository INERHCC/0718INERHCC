 175  mprofit(i,r) = vim(i,r) - sum(s, pvxmd(i,s,r)*vxmd(i,s,r)+sum(j, vtwr(j,i,s,r))*pvtwr(i,s,r));
 176  mprofit(i,r) = round(mprofit(i,r),5);
 177  display mprofit;
 178   
 179  yprofit(g,r) = vom(g,r)*(1-rto(g,r))
 180          - sum(i, vdfm(i,g,r)*(1+rtfd0(i,g,r))
 181                 + vifm(i,g,r)*(1+rtfi0(i,g,r)))
 182          - sum(f, vfm(f,g,r)*(1+rtf0(f,g,r)));
 183   
 184  yprofit(g,r) = round(yprofit(g,r),6)

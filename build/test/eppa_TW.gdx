 109   
 110  *       Declare some intermediate arrays which are required to
 111  *       evaluate tax rates:
 112   
 113  parameter       vdm(g,r)        Aggregate demand for domestic output,
 114                  vom(g,r)        Total supply at market prices;
 115   
 116  vdm(i,r) = sum(g, vdfm(i,g,r));
 117  vom(i,r) = vdm(i,r) + sum(s, vxmd(i,r,s)) + vst(i,r);
 118   
 119  parameter
 120          rtf0(f,g,r)     Primary factor and commodity rates taxes
 121          rtfd0(i,g,r)    Firms domestic tax rates
 122          rtfi0(i,g,r)    Firms' import tax rates
 123          rtxs0(i,r,s)    Export subsidy rates
 124          rtms0(i,r,s)    Import taxes rates;
 125   
 126  rtf0(f,g,r) = rtf(f,g,r);
 127  rtfd0(i,g,r) = rtfd(i,g,r);
 128  rtfi0(i,g,r) = rtfi(i,g,r);
 129  rtxs0(i,r,s) = rtxs(i,r,s);
 130  rtms0(i,r,s) = rtms(i,r,s);
 131   
 132  parameter       pvxmd(i,s,r)    Import price (power of benchmark tariff)
 133                  pvtwr(i,s,r)    Import price for transport services;
 134   
 135  pvxmd(i,s,r) = (1+rtms0(i,s,r)) * (1-rtxs0(i,s,r));
 136  pvtwr(i,s,r) = 1+rtms0(i,s,r);
 137   
 138  parameter
 139          vtw(j)          Aggregate international transportation services,
 140          vpm(r)          Aggregate private demand,
 141          vgm(r)          Aggregate public demand,
 142          vim(i,r)        Aggregate imports,
 143          evom(f,r)       Aggregate factor endowment at market prices,
 144          vb(*)           Current account balance;
 145   
 146  vtw(j) = sum(r, vst(j,r));
 147  vom("c",r) = sum(i, vdfm(i,"c",r)*(1+rtfd0(i,"c",r)) + vifm(i,"c",r)*(1+rtfi0(i,"c",r)))/(1-rto("c",r));
 148  vom("g",r) = sum(i, vdfm(i,"g",r)*(1+rtfd0(i,"g",r)) + vifm(i,"g",r)*(1+rtfi0(i,"g",r)))/(1-rto("g",r));
 149  vom("i",r) = sum(i, vdfm(i,"i",r)*(1+rtfd0(i,"i",r)) + vifm(i,"i",r)*(1+rtfi0(i,"i",r)))/(1-rto("i",r));
 150   
 151  vdm("c",r) = vom("c",r);
 152  vdm("g",r) = vom("g",r);
 153  vim(i,r) =  sum(g, vifm(i,g,r));
 154  evom(f,r) = sum(g, vfm(f,g,r));
 155  vb(r) = vom("c",r) + vom("g",r) + vom("i",r)
 156          - sum(f, evom(f,r))
 157          - sum(g,  vom(g,r)*rto(g,r))
 158          - sum(g,  sum(i, vdfm(i,g,r)*rtfd(i,g,r) + vifm(i,g,r)*rtfi(i,g,r)))
 159          - sum(g,  sum(f, vfm(f,g,r)*rtf(f,g,r)))
 160          - sum((i,s), rtms(i,s,r) *  (vxmd(i,s,r) * (1-rtxs(i,s,r)) + sum(j,vtwr(j,i,s,r))))
 161          + sum((i,s), rtxs(i,r,s) * vxmd(i,r,s));
